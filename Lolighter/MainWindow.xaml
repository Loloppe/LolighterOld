<Window x:Class="Lolighter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Lolighter" Height="300" Width="800" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <Grid Margin="0,0,-6,0" Width="800">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="47*"/>
            <ColumnDefinition Width="53*"/>
        </Grid.ColumnDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="271" Margin="224,0,0,0" VerticalAlignment="Top" Width="200" Grid.Column="1"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="271" Margin="24,0,0,0" VerticalAlignment="Top" Width="200" Grid.Column="1"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="271" Margin="200,0,0,0" VerticalAlignment="Top" Width="200" Grid.ColumnSpan="2"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="271" VerticalAlignment="Top" Width="200"/>
        <Button x:Name="OpenFile" Content="Open" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="68" Click="OpenFile_Click" IsDefault="True"/>
        <Button x:Name="LoloppeGenerator" Content="Loloppe" HorizontalAlignment="Left" Margin="201,134,0,0" VerticalAlignment="Top" Width="199" Height="69" Click="LoloppeGenerator_Click" IsEnabled="False" RenderTransformOrigin="0.82,0.46" Grid.ColumnSpan="2"/>
        <Button x:Name="SimpleLighter" Content="Light" HorizontalAlignment="Left" Margin="201,0,0,0" VerticalAlignment="Top" Width="199" Height="68" Click="SimpleLighter_Click" IsEnabled="False" Grid.ColumnSpan="2"/>
        <Button x:Name="SlidersMadness" Content="Sliders" HorizontalAlignment="Left" Margin="0,68,0,0" VerticalAlignment="Top" Width="200" Height="68" Click="SlidersMadness_Click" IsEnabled="False"/>
        <Button x:Name="InvertedMadness" Content="Inverted" HorizontalAlignment="Left" Margin="201,68,0,0" VerticalAlignment="Top" Width="199" Height="69" Click="InvertedMadness_Click" IsEnabled="False" Grid.ColumnSpan="2"/>
        <Button x:Name="BombGenerator" Content="Bomb" HorizontalAlignment="Left" Margin="0,135,0,0" VerticalAlignment="Top" Width="200" Height="68" Click="BombGenerator_Click" IsEnabled="False"/>
        <Button x:Name="SaveFile" Content="Save" HorizontalAlignment="Left" Margin="201,203,0,0" VerticalAlignment="Top" Width="199" Height="68" Click="SaveFile_Click" IsEnabled="False" RenderTransformOrigin="0.82,0.46" Grid.ColumnSpan="2"/>
        <Button x:Name="Converter" Content="Converter" HorizontalAlignment="Left" Margin="0,203,0,0" VerticalAlignment="Top" Width="200" Height="68" Click="Converter_Click" IsEnabled="True" RenderTransformOrigin="0.82,0.46"/>
        <Label Content="Limiter (in beat)" HorizontalAlignment="Left" Margin="254,59,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <Label Content="New file name" HorizontalAlignment="Left" Margin="252,191,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <TextBox x:Name="Limiter" HorizontalAlignment="Left" Height="23" Margin="256,85,0,0" TextWrapping="Wrap" Text="0.25" VerticalAlignment="Top" Width="139" Grid.Column="1"/>
        <TextBox x:Name="FileName" HorizontalAlignment="Left" Height="23" Margin="254,218,0,0" TextWrapping="Wrap" Text="ExpertPlusStandard" VerticalAlignment="Top" Width="139" RenderTransformOrigin="0.189,0.174" Grid.Column="1"/>
        <Label Content="Offset (in beat)" HorizontalAlignment="Left" Margin="45,215,0,0" VerticalAlignment="Top" FontSize="10" Grid.Column="1"/>
        <TextBox x:Name="ColorOffset" HorizontalAlignment="Left" Height="23" Margin="44,234,0,0" TextWrapping="Wrap" Text="0.00" VerticalAlignment="Top" Width="77" Grid.Column="1"/>
        <Label Content="Speed (in beat)" HorizontalAlignment="Left" Margin="44,177,0,0" VerticalAlignment="Top" FontSize="10" Grid.Column="1"/>
        <TextBox x:Name="ColorSwapSpeed" HorizontalAlignment="Left" Height="23" Margin="44,196,0,0" TextWrapping="Wrap" Text="4.00" VerticalAlignment="Top" Width="77" Grid.Column="1"/>
        <CheckBox x:Name="AllowLimiter" HorizontalAlignment="Left" Margin="376,65,0,0" VerticalAlignment="Top" Width="17" IsChecked="True" Grid.Column="1"/>
        <Label Content="Modifier" HorizontalAlignment="Left" Margin="280,15,0,0" VerticalAlignment="Top" FontWeight="Bold" Width="85" FontSize="18" Grid.Column="1"/>
        <Label Content="General" HorizontalAlignment="Left" Margin="284,152,0,0" VerticalAlignment="Top" FontWeight="Bold" Width="74" FontSize="18" Grid.Column="1"/>
        <Label Content="Back/Top" HorizontalAlignment="Left" Margin="61,66,0,0" VerticalAlignment="Top" FontSize="12" Width="60" Grid.Column="1"/>
        <Label Content="Use Fade" HorizontalAlignment="Left" Margin="61,130,0,0" VerticalAlignment="Top" FontSize="12" Grid.Column="1"/>
        <Label Content="Spin/Zoom" HorizontalAlignment="Left" Margin="61,145,0,0" VerticalAlignment="Top" FontSize="12" Grid.Column="1"/>
        <CheckBox x:Name="AllowBackStrobe" HorizontalAlignment="Left" Margin="44,71,0,0" VerticalAlignment="Top" Width="17" IsChecked="True" Grid.Column="1"/>
        <CheckBox x:Name="AllowFade" HorizontalAlignment="Left" Margin="44,136,0,0" VerticalAlignment="Top" Width="17" IsChecked="True" Grid.Column="1"/>
        <CheckBox x:Name="AllowSpin" HorizontalAlignment="Left" Margin="44,151,0,0" VerticalAlignment="Top" Width="17" IsChecked="True" Grid.Column="1"/>
        <Label Content="Spin Speed" HorizontalAlignment="Left" Margin="140,186,0,0" VerticalAlignment="Top" FontWeight="Bold" Grid.Column="1"/>
        <Label Content="Slow" HorizontalAlignment="Left" Margin="140,209,0,0" VerticalAlignment="Top" FontSize="10" Grid.Column="1"/>
        <Label Content="Fast" HorizontalAlignment="Left" Margin="140,233,0,0" VerticalAlignment="Top" FontSize="10" Grid.Column="1"/>
        <TextBox x:Name="FastMinSpinSpeed" HorizontalAlignment="Left" Height="19" Margin="167,235,0,0" TextWrapping="Wrap" Text="8" VerticalAlignment="Top" Width="19" Grid.Column="1"/>
        <TextBox x:Name="FastMaxSpinSpeed" HorizontalAlignment="Left" Height="19" Margin="187,235,0,0" TextWrapping="Wrap" Text="13" VerticalAlignment="Top" Width="19" Grid.Column="1"/>
        <TextBox x:Name="SlowMinSpinSpeed" HorizontalAlignment="Left" Height="19" Margin="167,212,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="19" Grid.Column="1"/>
        <TextBox x:Name="SlowMaxSpinSpeed" HorizontalAlignment="Left" Height="19" Margin="187,212,0,0" TextWrapping="Wrap" Text="8" VerticalAlignment="Top" Width="19" Grid.Column="1"/>
        <Label Content="Bottom/Back/Side" HorizontalAlignment="Left" Margin="61,97,0,0" VerticalAlignment="Top" FontSize="12" Width="108" Grid.Column="1"/>
        <CheckBox x:Name="AllowSideStrobe" HorizontalAlignment="Left" Margin="44,103,0,0" VerticalAlignment="Top" Width="17" IsChecked="True" RenderTransformOrigin="2.686,0.444" Grid.Column="1"/>
        <Label Content="Neon Ring" HorizontalAlignment="Left" Margin="61,81,0,0" VerticalAlignment="Top" FontSize="12" Grid.Column="1"/>
        <CheckBox x:Name="AllowNeonStrobe" HorizontalAlignment="Left" Margin="44,87,0,0" VerticalAlignment="Top" Width="17" IsChecked="True" Grid.Column="1"/>
        <Label Content="Strobes" HorizontalAlignment="Left" Margin="44,46,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="12" Grid.Column="1"/>
        <Label Content="Light" HorizontalAlignment="Left" Margin="98,15,0,0" VerticalAlignment="Top" FontWeight="Bold" Width="55" FontSize="18" Grid.Column="1"/>
        <Label Content="Other Features" HorizontalAlignment="Left" Margin="44,113,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="12" Grid.Column="1"/>
        <Label Content="Color Swap" HorizontalAlignment="Left" Margin="44,162,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="12" Grid.Column="1"/>
    </Grid>
</Window>
